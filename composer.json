{
    "name": "gortonsd/Marshal",
    "description": "A lightweight PHP framework with automatic controller discovery, route registration via docblock annotations, and intelligent route caching",
    "type": "library",
    "keywords": [
        "router",
        "php",
        "routing",
        "framework",
        "controller",
        "annotations",
        "cache",
        "lightweight"
    ],
    "homepage": "https://github.com/gortonsd/marshal",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Gorton",
            "email": "dev@gorton.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "gortonsd\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gortonsd\\Router\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/gortonsd/router/issues",
        "source": "https://github.com/gortonsd/router"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/gortonsd"
        }
    ]
}
